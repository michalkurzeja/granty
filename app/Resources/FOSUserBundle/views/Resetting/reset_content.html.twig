{% trans_default_domain 'FOSUserBundle' %}

<div class="row">
    <div class="callout">
        {{ form_start(form, { 'action': path('fos_user_resetting_reset', {'token': token}), 'attr': { 'class': 'fos_user_resetting_reset' } }) }}
            {{ form_widget(form) }}
            <div>
                <button class="button" type="submit">{{ 'resetting.reset.submit'|trans }}</button>
            </div>
        {{ form_end(form) }}
    </div>
</div>
